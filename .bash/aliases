# /etc/profile.d/
alias ..='cd ..'
alias .casing='xdotool key Caps_Lock'
alias .cleanup='pc -Sc'
alias .gcc='gcc -std=c99 -pedantic -Wall -Wextra -Werror -Wshadow -Wunused -Wconversion -Wformat=2 -g -O0 -o'
alias .hist='history | grep '
alias .journal='journalctl -p 3 -b'
alias .mirrors='sudo reflector --verbose --country "France,Germany,United Kingdom,Netherlands,Belgium,Luxembourg,Ireland,Austria,Switzerland" --age 12 --protocol https --sort rate --save /etc/pacman.d/mirrorlist --fastest 8'
alias .mount_pass='/home/devid/.local/bin/mount-password-store'
alias .orphans='pacman -Qdtq'
alias .pc-integrity='pacman -Qkk'
alias .recompile='ghc --make $HOME/.config/xmonad/xmonad.hs -i -ilib -dynamic -fforce-recomp -o $HOME/.config/xmonad/xmonad-x86_64-linux'
alias .sizing='sudo du -h -x / | grep -E "[0-9\.]+G|[5-9][0-9][0-9]M" | sort -rh'
alias .sshP='eval "$(ssh-agent -s)" && ssh-add ~/.ssh/id_ed25519_github'
alias .timeshift='sudo timeshift-gtk'
alias .upload='curl bashupload.com -T '
alias .vmware='sudo sysctl vm.compaction_proactiveness=0 && vmware'
alias .vpn='sudo openvpn --config /etc/openvpn/client/nl-free-162.protonvpn.tcp.ovpn --script-security 2 --up /etc/openvpn/client/up --down /etc/openvpn/client/down'
alias .prot='cd /home/devid/.local/bin/lolzer/ && oniux python /home/devid/.local/bin/lolzer/py.py &'
alias .xev='xev | grep keycode'
alias .xprop='xprop | grep WM_CLASS'
alias C='xclip -selection clipboard'
alias c='\cat'
alias cat='bat --number'
alias cheat='cheat -p personal '
alias doom="~/.emacs.d/bin/doom"
alias grep='grep --color=auto'
alias halt='poweroff'
alias j='zi'
alias lsblk='lsblk -o NAME,SIZE,FSTYPE,LABEL,MOUNTPOINT,UUID,MODEL,SERIAL'
alias nano='vim'
alias paru='yay'
alias pc='sudo pacman'
alias psoccreator='WINEDLLOVERRIDES=mshtml=d wine psoccreator_4.4_Windows_x86-x64.exe'
alias sd='sudo '
alias sr='source'
alias st='git status'
alias sys='sudo systemctl'
alias v='nvim'
alias vi='\vim'
alias vim='nvim'
alias dS='less ~/.local/bin/lolzer/an'
