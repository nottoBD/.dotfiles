# /etc/profile.d/
alias ..='cd ..'
alias C='xclip -selection clipboard'
alias .casing='xdotool key Caps_Lock'
alias .cleanup='bleachbit'
alias .mirrors='sudo reflector --verbose --country "France,Germany,United Kingdom,Netherlands,Belgium,Luxembourg,Ireland,Austria,Switzerland" --age 12 --protocol https --sort rate --save /etc/pacman.d/mirrorlist --fastest 8'
alias .recompile='ghc --make $HOME/.config/xmonad/xmonad.hs -i -ilib -dynamic -fforce-recomp -o $HOME/.config/xmonad/xmonad-x86_64-linux'
alias .sizing='sudo du -h / | grep "[0-9\.]\+G\|[5-9][0-9][0-9]M" | sort -rh'
alias .xev='xev | grep keycode'
alias .xprop='xprop | grep WM_CLASS'
alias cat='bat --number'
alias cheat='cheat -c '
alias doom="~/.emacs.d/bin/doom"
alias emacs="emacsclient -a '' -c"
alias grep='grep --color=auto'
alias halt='poweroff'
alias nano='vim'
alias paru='yay'
alias psoccreator='WINEDLLOVERRIDES=mshtml=d wine psoccreator_4.4_Windows_x86-x64.exe'
alias s='sudo'
alias sshP='eval "$(ssh-agent -s)" && ssh-add ~/.ssh/id_ed25519_github'
alias st='git status'
alias v='vim'
alias vi='vim'
alias vmware='sudo sysctl vm.compaction_proactiveness=0 && vmware'
