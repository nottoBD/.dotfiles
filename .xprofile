pactl set-sink-mute @DEFAULT_SINK@ 1
#xautolock -time 4 -locker "xtrlock" &
xidlehook --not-when-audio --timer 240 "if pactl list sinks | grep 'RUNNING' >/dev/null && pactl list sinks | grep -q 'Volume.*[1-9][0-9]%\|Volume.*100%'; then exit 0; else xtrlock; fi" '' --timer 2400 'systemctl suspend' '' &
xset b off
xset s off
xset s noblank
xset r rate 280 80
xset dpms 2900 4100 4900
export PATH="$PATH:$HOME/.config/emacs/bin:/usr/local/sbin:/usr/local/bin:/usr/bin:/usr/bin/site_perl:/usr/bin/vendor_perl:/usr/bin/core_perl:/usr/local/go/bin:/home/devid/go/bin:/home/devid/.local/bin:/home/devid/.config/dmscripts/scripts:/home/devid/.cron"
export QT_QPA_PLATFORMTHEME=qt6ct
export QT_STYLE_OVERRIDE=kvantum
